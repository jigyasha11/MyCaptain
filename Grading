#include<stdio.h>
int main()
{
    int m;
    printf("Enter marks: ");
    scanf("%d",&m);
    if(m<=100 && m>=85)
    printf("Grade A");
    else if(m<85 && m>=70)
    printf("Grade B");
    else if(m<70 && m>=55)
    printf("Grade C");
    else if(m<55 && m>=40)
    printf("Grade D");
    else if(m<40)
    printf("Grade E");
    return 0;
}
